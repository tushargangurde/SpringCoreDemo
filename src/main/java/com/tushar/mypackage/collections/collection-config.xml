<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.tushar.mypackage.collections.Employee" name="employee">

		<property name="employeeName" value="Tushar" />

		<property name="employeePhones">
			<list>
				<value>468545</value>
				<value>54566</value>
				<value>596656</value>
			</list>
		</property>

		<property name="employeeAddresses">
			<set>
				<value>Navi Mumbai</value>
				<value>Nashik</value>
			</set>
		</property>

		<property name="employeeCourses">
			<map>
				<entry key="Java" value="2 months" />
				<entry key="Python" value="3 months" />
				<entry key="C" value="1 months" />
			</map>
		</property>

	</bean>

</beans>